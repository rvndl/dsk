generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Share {
  slug            String   @id @default(cuid())
  name            String
  path            String
  downloads       Int      @default(0)
  shared          Boolean  @default(false)
  sharesRemaining Int      @default(0)
  createdAt       DateTime @default(now())
}
